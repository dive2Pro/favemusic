{
    "extends": "airbnb",
    "rules": {
        "react/prop-types": 0,
        "react/jsx-no-bind": 0,
        "camelcase": 0,
        "default-case": 0,
        "comma-dangle": 0,
        "consistent-return": 0,
        "no-new": 0,
        "prefer-template": 0,
        "quotes": 0,
        "new-cap": 0,
        "no-else-return": 0,
        "semi": 0,
        "max-len": [1, 120, 2, {
            "ignoreComments": true
        }],
        "no-unused-expressions":"off",
        "arrow-body-style":"off",
        "prefer-const":["error",{
             "destructuring": "any",
             "ignoreReadBeforeAssign": false
        }],
        "one-var":"off",
        "no-underscore-dangle":"off",
        "global-require":"off",
        "no-console":"off"
    },
    "react/display-name": [0, {
        "ignoreTranspilerName": true
    }]
}